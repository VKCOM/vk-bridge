{
  "name": "@vkontakte/vk-bridge",
  "version": "2.7.3",
  "description": "Connects a Mini App with VK client",
  "license": "MIT",
  "main": "dist/index.js",
  "browser": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "umdName": "vkBridge",
  "types": "dist/types/src/index.d.ts",
  "typesVersions": {
    "<4.0": {
      "*": [
        "dist/types3.8.3/src/index.d.ts"
      ]
    }
  },
  "scripts": {
    "watch": "rollup -c -w",
    "build": "rimraf dist && NODE_ENV=production rollup -c && yarn build:legacy-types",
    "build:legacy-types": "rollup -c rollup.config-legacy-types.mjs >/dev/null",
    "prepublishOnly": "npm run build",
    "pretest": "yarn build",
    "test": "jest",
    "deduplicate": "yarn-deduplicate --list && yarn-deduplicate --fail"
  },
  "author": {
    "name": "VK",
    "url": "https://vk.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VKCOM/vk-bridge.git"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.5",
    "jest": "^29.5.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "yarn-deduplicate": "^6.0.2"
  },
  "engines": {
    "yarn": "^1.21.1",
    "node": ">=14.18.0"
  },
  "packageManager": "yarn@1.22.19"
}
